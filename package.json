{
  "name": "docsify-pandoc",
  "version": "0.0.1",
  "description": "Convert your docsify docs to almost any format using pandoc",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "bin": {
    "docsify-pandoc": "./.bin/cli.js"
  },
  "scripts": {
    "lint": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtuKILL/docsify-pandoc.git"
  },
  "keywords": [
    "docsify",
    "docs",
    "pandoc"
  ],
  "author": "archurrito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtuKILL/docsify-pandoc/issues"
  },
  "homepage": "https://github.com/ArtuKILL/docsify-pandoc#readme",
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.12",
    "@types/shelljs": "^0.8.15",
    "@types/which": "^3.0.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "esm": "^3.2.25",
    "mustache": "^4.2.0",
    "shelljs": "^0.8.5",
    "which": "^4.0.0"
  }
}
